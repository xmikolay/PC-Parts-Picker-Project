<Window x:Class="OODProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OODProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen" MinHeight="350" MinWidth="700">

    <Grid>
        <Label Content="CPU" HorizontalAlignment="Left" Margin="60,73,0,0" VerticalAlignment="Top"/>
        <Label Content="Motherboard" HorizontalAlignment="Left" Margin="60,104,0,0" VerticalAlignment="Top"/>
        <Label Content="RAM" HorizontalAlignment="Left" Margin="60,135,0,0" VerticalAlignment="Top"/>
        <Label Content="GPU" HorizontalAlignment="Left" Margin="62,166,0,0" VerticalAlignment="Top"/>
        <Label Content="Power Supply" HorizontalAlignment="Left" Margin="60,197,0,0" VerticalAlignment="Top"/>
        <Label Content="Case" HorizontalAlignment="Left" Margin="60,223,0,0" VerticalAlignment="Top"/>
        <Label Content="Storage 1" HorizontalAlignment="Left" Margin="62,254,0,0" VerticalAlignment="Top"/>
        <Label Content="Storage 2*" HorizontalAlignment="Left" Margin="62,285,0,0" VerticalAlignment="Top"/>
        <Label Content="CPU Cooler*" HorizontalAlignment="Left" Margin="57,316,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkCPU" HorizontalAlignment="Left" Margin="182,78,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkMB" HorizontalAlignment="Left" Margin="182,109,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkRAM" HorizontalAlignment="Left" Margin="182,140,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkGPU" HorizontalAlignment="Left" Margin="182,171,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkPSU" HorizontalAlignment="Left" Margin="182,203,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkCase" HorizontalAlignment="Left" Margin="182,229,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkStorage1" HorizontalAlignment="Left" Margin="182,259,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkStorage2" HorizontalAlignment="Left" Margin="182,289,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tblkCooler" HorizontalAlignment="Left" Margin="182,321,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <Button x:Name="btnCPU" Content="Browse" HorizontalAlignment="Left" Margin="283,76,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnMotherboard" Content="Browse" HorizontalAlignment="Left" Margin="283,107,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnRAM" Content="Browse" HorizontalAlignment="Left" Margin="283,138,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnGPU" Content="Browse" HorizontalAlignment="Left" Margin="283,169,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnPSU" Content="Browse" HorizontalAlignment="Left" Margin="283,200,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnCase" Content="Browse" HorizontalAlignment="Left" Margin="283,229,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnStorage1" Content="Browse" HorizontalAlignment="Left" Margin="283,257,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnStorage2" Content="Browse" HorizontalAlignment="Left" Margin="283,288,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnCooler" Content="Browse" HorizontalAlignment="Left" Margin="283,319,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
